#include <FastLED.h>
#define LED_PIN     13
#define NUM_LEDS    232
#define BRIGHTNESS  60
CRGB leds[NUM_LEDS];

int leftTurn = A4;
int rightTurn = A6;

int leftTurnVal = 0;
int rightTurnVal = 0;

void setup() {
  delay(500);
  Serial.begin(9600);
